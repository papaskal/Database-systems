<!DOCTYPE html>
<html>

<head>
    <link rel="stylesheet" href="/app.css">
    <title>Young teachers who borrowed most books</title>
</head>

<body>
    <h1>Teachers</h1>
    <table>
        <tr>
            <th>Username</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Number of Borrowed Books</th>
        </tr>
        <% for (const teacher of teachers) { %>
            <tr>
                <td>
                    <%= teacher.username %>
                </td>
                <td>
                    <%= teacher.first_name %>
                </td>
                <td>
                    <%= teacher.last_name %>
                </td>
                <td>
                    <%= teacher.borrow_count %>
                </td>
            </tr>
            <% } %>
    </table>
</body>

</html>